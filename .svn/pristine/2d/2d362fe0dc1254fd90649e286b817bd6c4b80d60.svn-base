package dcmm;

public class Timeval {
    private long tvSec;
    private long tvUsec;

    public Timeval(long tvSec, long tvUsec) {
        this.tvSec = tvSec;
        this.tvUsec = tvUsec;
    }

    public Timeval() {}

    public long getTvSec() {
        return this.tvSec;
    }

    public long getTvUsec() {
        return this.tvUsec;
    }

    public void setTvSec(long tvSec) {
        this.tvSec = tvSec;
    }

    public void setTvUsec(long tvUsec) {
        this.tvUsec = tvUsec;
    }
}

