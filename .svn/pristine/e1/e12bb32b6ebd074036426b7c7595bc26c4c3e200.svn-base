package dcmm;

public class DcmmAddr {
    private int port;
    private String ip = "";

    public DcmmAddr(int port, String ip) {
        this.port = port;
        this.ip = ip;
    }

    public DcmmAddr() {}

    public int getPort() {
        return this.port;
    }

    public String getIp() {
        return this.ip;
    }

    public void setPort(int port) {
        this.port = port;
    }

    public void setIp(String ip) {
        this.ip = ip;
    }
}


